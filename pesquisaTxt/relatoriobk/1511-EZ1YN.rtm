      object daSQL1: TdaSQL
        GuidCollationType = gcMSAccess
        DatabaseName = 'conHabil'
        DatabaseType = dtMSAccess
        DataPipelineName = 'Almox_Solicitacoes_Cabecalho'
        IsCaseSensitive = True
        LinkColor = clMaroon
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT Almox_Solicitacoes_Cabecalho.Codigo, '
          '       Almox_Solicitacoes_Cabecalho.Codigo_Funcionario, '
          '       Almox_Solicitacoes_Cabecalho.Data, '
          '       Almox_Solicitacoes_Cabecalho.Status, '
          '       Almox_Solicitacoes_Cabecalho.Motivo, '
          '       Almox_Solicitacoes_Cabecalho.Setor, '
          '       Almox_Solicitacoes_Detalhes.Codigo_Item, '
          '       Almox_Solicitacoes_Detalhes.Quantidade, '
          '       Funcionario.Nome, Almox_Itens.Descricao, '
          '       Almox_Itens.Quantidade AS Quantidade_2, '
          '       Almox_Itens.Custo, '
          
            '       iif ( Almox_Solicitacoes_Cabecalho.Status = '#39'P'#39', Almox_It' +
            'ens.Quantidade - Almox_Solicitacoes_Detalhes.Quantidade, 0) AS i' +
            'if_Almox_Solicitacoes_Ca, '
          
            '       Almox_Itens.Custo * Almox_Itens.Quantidade AS Almox_Itens' +
            '_Custo_Almox_I'
          'FROM ((('
          '     Almox_Solicitacoes_Cabecalho Almox_Solicitacoes_Cabecalho'
          
            '      INNER JOIN Almox_Solicitacoes_Detalhes Almox_Solicitacoes_' +
            'Detalhes ON '
          
            '     (Almox_Solicitacoes_Detalhes.Empresa = Almox_Solicitacoes_C' +
            'abecalho.Empresa)'
          
            '      AND (Almox_Solicitacoes_Detalhes.Codigo_Cabecalho = Almox_' +
            'Solicitacoes_Cabecalho.Codigo) )'
          '      LEFT OUTER JOIN Funcionario Funcionario ON '
          
            '     (Funcionario.Codigo = Almox_Solicitacoes_Cabecalho.Codigo_F' +
            'uncionario)'
          
            '      AND (Funcionario.Empresa = Almox_Solicitacoes_Cabecalho.Em' +
            'presa) )'
          '      INNER JOIN Almox_Itens Almox_Itens ON '
          
            '     (Almox_Itens.Codigo = Almox_Solicitacoes_Detalhes.Codigo_It' +
            'em)'
          
            '      AND (Almox_Itens.Empresa = Almox_Solicitacoes_Detalhes.Emp' +
            'resa) )'
          'WHERE '
          
            '      ( Almox_Solicitacoes_Cabecalho.Codigo BETWEEN 0 AND 999999' +
            ' )'
          'ORDER BY Almox_Solicitacoes_Cabecalho.Empresa, '
          '         Almox_Solicitacoes_Cabecalho.Codigo, '
          '         Almox_Solicitacoes_Detalhes.Codigo')
