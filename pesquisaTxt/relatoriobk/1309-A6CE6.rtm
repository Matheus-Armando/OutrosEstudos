          '       Sequencial.Quantidade_Atual, '
          '       Sequencial.Descri'#231#227'o AS Descri'#231#227'o_2, '
          '       Sequencial.Pre'#231'o_Venda AS Pre'#231'o_Venda_2, '
          
            '       Produto.Digito_Verificador & " - " & Produto.Descri'#231#227'o AS' +
            ' Produto_Digito_Verificado, '
          
            '       Produto_Grades.Codigo & " - " & Produto_Grades.Descri'#231#227'o ' +
