      object daSQL1: TdaSQL
        GuidCollationType = gcMSAccess
        DatabaseName = 'conHabil'
        DatabaseType = dtMSAccess
        DataPipelineName = 'Lctos_Antecipados'
        IsCaseSensitive = True
        LinkColor = clMaroon
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT Lctos_Antecipados.Codigo, '
          '       Lctos_Antecipados.Empresa, '
          '       Lctos_Antecipados.Lcto_Para, '
          '       Lctos_Antecipados.Tipo, '
          '       Lctos_Antecipados.Codigo_Pessoa, '
          '       Lctos_Antecipados.Codigo_ContaCaixa, '
          '       Lctos_Antecipados.Codigo_CentroCusto, '
          '       Lctos_Antecipados.Descricao, '
          '       Lctos_Antecipados.Data, '
          '       Lctos_Antecipados.Valor, '
          '       Lctos_Antecipados.Referencia, '
          '       Lctos_Antecipados.Origem, '
          '       Lctos_Antecipados.Codigo_Origem, '
          
            '       ( select F.Nome_Fantasia from Fornecedor F where F.Codigo' +
            ' =  Lctos_Antecipados.Codigo_Pessoa and F.Empresa = Lctos_Anteci' +
            'pados.Empresa )  AS select_F_Nome_Fantasia_f, '
          
            '       Caixa_Contas.[C'#243'digo_Conta] & " - " & Caixa_Contas.[Descr' +
            'i'#231#227'o] AS Caixa_Contas_C'#243'digo_Conta, '
          
            '       iif(Lctos_Antecipados.Tipo = 0, Lctos_Antecipados.Valor, ' +
            'Lctos_Antecipados.Valor * (-1)) AS iif_Lctos_Antecipados_Tip'
          'FROM (Lctos_Antecipados Lctos_Antecipados'
          '      LEFT OUTER JOIN Caixa_Contas Caixa_Contas ON '
          '     (Caixa_Contas.Empresa = Lctos_Antecipados.Empresa)'
          
            '      AND (Caixa_Contas.Codigo = Lctos_Antecipados.Codigo_ContaC' +
            'aixa) )'
          'WHERE ( Lctos_Antecipados.Lcto_Para = 1 )'
          
            '       AND ( Lctos_Antecipados.Data BETWEEN #2015-01-01 00.00.00' +
            '# AND #2015-12-31 00.00.00# )'
          
            '       AND ( Lctos_Antecipados.Codigo_Pessoa BETWEEN 1 AND 99999' +
            '9 )')
        SQLType = sqSQL2
