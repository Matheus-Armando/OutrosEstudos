      object daSQL1: TdaSQL
        GuidCollationType = gcMSAccess
        DatabaseName = 'conHabil'
        DatabaseType = dtMSAccess
        DataPipelineName = 'Contas_Receber'
        IsCaseSensitive = True
        LinkColor = clMaroon
        LinkType = ltMagicSQL
        MaxSQLFieldAliasLength = 25
        SQLText.Strings = (
          'SELECT Contas_Receber.Codigo, '
          '       Contas_Receber.Documento, '
          '       Contas_Receber.Data_Conta, '
          '       Contas_Receber.Data_Vencimento, '
          '       Contas_Receber.Valor, '
          '       Contas_Receber.C'#243'digo_Cliente, '
          '       Empresa.Nome_Fantasia, '
          '       Empresa.Raz'#227'o_Social, Empresa.Endere'#231'o, '
          '       Empresa.Bairro, Empresa.Cidade, '
          '       Empresa.UF, Empresa.CEP, '
          '       Empresa.Respons'#225'vel, Empresa.Telefone, '
          '       Empresa.FAX, Empresa.N'#250'mero, '
          '       Empresa.Complemento, '
          
            '       Int( Date() - Contas_Receber.Data_Vencimento ) AS Int_Dat' +
            'e_Contas_Receber_D, '
          
            '       (Select UCase( c.Cliente_Nome ) from cliente c where c.co' +
            'digo  = contas_receber.c'#243'digo_cliente and c.empresa = contas_rec' +
            'eber.empresa) AS Select_UCase_c_Cliente_N, '
          
            '       ( select Taxa_Juros from configuracao_global ) AS select_' +
            'Taxa_Juros_from_c, '
          
            '       ( select Tipo_Juros from configuracao_global ) AS select_' +
            'Tipo_Juros_from_c, '
          
            '       ( select Dias_Car'#234'ncia_Juros from configuracao_global ) A' +
            'S select_Dias_Car'#234'ncia_Jur, '
          
            '       ( select Incluir_Car'#234'ncia_Na_Cobran'#231'a from configuracao_g' +
            'lobal ) AS select_Incluir_Car'#234'ncia, '
          
            '       ( select Percentual_Multa_Atraso from configuracao_global' +
            '  ) AS select_Percentual_Multa, '
          
            '       ( select Dias_Atraso_Multa from configuracao_global ) AS ' +
            'select_Dias_Atraso_Multa'
          'FROM (Contas_Receber Contas_Receber'
          '      INNER JOIN Empresa Empresa ON '
          '     (Empresa.Codigo = Contas_Receber.Empresa) )'
          'WHERE ( Contas_Receber.Cancelado <> TRUE )'
          '       AND ( Contas_Receber.Data_Pagamento IS NULL  )'
          
            '       AND ( Contas_Receber.C'#243'digo_Cliente BETWEEN 1 AND 999999 ' +
            ')'
          
            '       AND ( Int( Date() - Contas_Receber.Data_Vencimento ) >= 0' +
            ' )'
          'ORDER BY Contas_Receber.C'#243'digo_Cliente, '
          '         Contas_Receber.Data_Vencimento, 20, '
          '         Contas_Receber.Data_Conta')
        SQLType = sqSQL2
