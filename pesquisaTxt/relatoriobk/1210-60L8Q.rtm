            '       ( Select Nome from funcionario where funcionario.codigo =' +
            ' Caixa_Gaveta.Funcion'#225'rio and funcionario.empresa = Caixa_Gaveta' +
            '.empresa)  AS Select_Nome_from_funcion, '
          
            '       Empresa.Raz'#227'o_Social & ", CNPJ " & Empresa.CGC AS Empresa' +
            '_Raz'#227'o_Social_CNPJ'
          'FROM (Caixa_Gaveta Caixa_Gaveta'
          '      INNER JOIN Empresa Empresa ON '
          '     (Empresa.Codigo = Caixa_Gaveta.Empresa) )'
          'WHERE ('#39'c'#39' <> '#39'c'#39' ) AND ('
          '      ( Caixa_Gaveta.Codigo BETWEEN 1 AND 999999 )'
